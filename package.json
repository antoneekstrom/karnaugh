  "scripts": {
    "clean": "rm public",
    "renderer:start": "parcel src/index.html -p 3000 --out-dir build --target electron --public-url \"./\"",
    "main:watch": "parcel watch src/main.js --out-file main.js --out-dir build --no-source-maps --target node",
    "main:start": "concurrently \"electron .\" && \"npm run renderer:watch\"",
    "start": "concurrently --kill-others \"cross-env BROWSER=none npm run main:start\" \"npm run renderer:start\"",
    "web:start": "parcel src/index.html",
    "renderer:build": "parcel build src/index.html --out-dir build --public-url ./",
    "main:build": "electron-builder",
    "build": "npm run clean && npm run main:build && npm run renderer:build"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-transition-group": "^4.4.0",
    "@types/sharp": "^0.25.1",
    "@svgr/parcel-plugin-svgr": "^5.4.0",
    "concurrently": "^5.2.0",
    "connected-react-router": "^6.8.0",
    "cross-env": "^7.0.2",
    "custom-electron-titlebar": "^3.2.2-hotfix62",
    "electron": "^9.1.0",
    "electron-builder": "^22.7.0",
    "electron-is-dev": "^1.2.0",
    "electron-rebuild": "^1.11.0",
    "immutable": "^4.0.0-rc.12",
    "parcel-bundler": "^1.12.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "sharp": "^0.25.4",
    "typescript": "^3.9.6"
  }
